<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <description>NLog target to stream events to an AWS Kinesis Stream</description>
    <authors>Connatix</authors>
    <projectUrl>https://github.com/Connatix/Connatix.NLogKinesisTarget</projectUrl>
    <iconUrl>http://i.connatix.com/logos/logo-black.png</iconUrl>
    <owners>Connatix</owners>
    <copyright>Copyright 2018 (c) Connatix. All rights reserved.</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
     <dependencies>
      <group targetFramework="netstandard2.0">
        <dependency id="AWSSDK.Core" version="3.3.25.1" exclude="Build,Analyzers" />  
        <dependency id="AWSSDK.Kinesis" version="3.3.6.4" exclude="Build,Analyzers" />    
        <dependency id="NLog" version="4.5.9" exclude="Build,Analyzers" />     
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$config$\netstandard2.0\$id$.dll" target="lib\netstandard2.0\$id$.dll" />
  </files>
</package>